<!DOCTYPE html>
<html>
  <head>
    <title>Foderstatistik</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body class="container">
    <div class="title" style="padding-top: 15px">
      <h1>Foderstatistik</h1>
    </div>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
      <div id="navbar-menu">
        <ul class="navbar-nav">
          <li class="nav-item" style="margin-right: 5px">
            <%= link_to 'Start', start_index_path, {class: "btn btn-outline-primary"} %>
          </li>
          <li class="nav-item" style="margin-right: 5px">
            <%= link_to 'Batchar', batches_path, {class: "btn btn-outline-primary"} %>
          </li>
          <li class="nav-item" style="margin-right: 5px">
            <%= link_to 'Utfodringar', consumptions_path, {class: "btn btn-outline-primary"} %>
          </li>
          <li class="nav-item" style="margin-right: 5px">
            <%= link_to 'Djur/djurgrupper', animal_groups_path, {class: "btn btn-outline-primary"} %>
          </li>
        </ul>
      </div>
    </nav>
    <%= yield %>
  </body>
</html>
