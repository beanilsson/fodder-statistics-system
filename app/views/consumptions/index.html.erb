<p id="notice"><%= notice %></p>

<h2>Utfodringar</h2>

<table>
  <thead>
    <tr>
      <th>Daum</th>
      <th>Mängd</th>
      <th>Batch</th>
      <th>Djurgrupp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consumptions.each do |consumption| %>
      <tr>
        <td><%= consumption.date %></td>
        <td><%= consumption.amount %></td>
        <td><%= consumption.batch %></td>
        <td><%= consumption.animal_group %></td>
        <td><%= link_to 'Visa', consumption %></td>
        <td><%= link_to 'Ändra', edit_consumption_path(consumption) %></td>
        <td><%= link_to 'Ta bort', consumption, method: :delete, data: { confirm: 'Är du säker?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'Start', start_index_path %>
<%= link_to 'Ny utfodring', new_consumption_path %>
